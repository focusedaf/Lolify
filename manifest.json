{
  "manifest_version": 3,
  "name": "Lolify",
  "version": "1.0",
  "description": "A meme extension that also tracks active tab URLs.",
  "permissions": ["tabs", "activeTab", "storage", "history"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-large.png"
  },
  "background": {
    "service_worker": "script.js"
  },
  "icons": {
    "128": "icon-large.png"
  }
}


